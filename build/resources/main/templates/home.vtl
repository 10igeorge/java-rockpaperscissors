<h1>Rock Paper Scissors</h1>
  <form action="/results">
    <div class="form-group">
      <label for="player">Player:</label>
      <input type="text" name="player" class="form-control" id="player">
    </div>
    <div class="form-group">
      <label for="comp">Computer:</label>
      <input type="text" class="form-control" id="comp">
    </div>
    <button id="play" type="submit" class="btn btn-info">Play!</button>
  </form>
